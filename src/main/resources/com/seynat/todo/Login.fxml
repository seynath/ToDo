<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="root" prefHeight="400.0" prefWidth="314.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.seynat.todo.LoginController">
   <children>
      <TextField fx:id="txtUsername" layoutX="83.0" layoutY="95.0" promptText="Enter Username" />
      <TextField fx:id="txtPassword" layoutX="83.0" layoutY="140.0" onAction="#txtPasswordOnAction" promptText="Enter Password" />
      <Button fx:id="btnLogin" layoutX="131.0" layoutY="175.0" mnemonicParsing="false" onAction="#btnLoginOnAction" text="LOG IN">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Label fx:id="lblRegister" layoutX="102.0" layoutY="212.0" onMouseClicked="#lblRegisterOnMouseClick" text="create a account" underline="true">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Label>
      <Label layoutX="110.0" layoutY="31.0" text="Login">
         <font>
            <Font size="29.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
