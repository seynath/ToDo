<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="root" prefHeight="400.0" prefWidth="339.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.seynat.todo.RegisterFormController">
   <children>
      <PasswordField fx:id="txtPassword" layoutX="86.0" layoutY="244.0" promptText="New Password" />
      <PasswordField fx:id="txtConfirmPassword" layoutX="86.0" layoutY="294.0" onAction="#txtConfirmPasswordOnAction" promptText="Confirm New Password" />
      <TextField fx:id="txtEmail" layoutX="87.0" layoutY="194.0" promptText="Email" />
      <TextField fx:id="txtUsername" layoutX="88.0" layoutY="148.0" promptText="Username" />
      <Button fx:id="btnRegister" layoutX="131.0" layoutY="335.0" mnemonicParsing="false" onAction="#btnRegisterOnAction" text="Register" />
      <Label layoutX="141.0" layoutY="14.0" text="Register" />
      <Label fx:id="lblIncorrect1" layoutX="147.0" layoutY="270.0" text="Password Incorrect!" textFill="RED">
         <font>
            <Font size="10.0" />
         </font>
      </Label>
      <Label fx:id="lblIncorrect2" layoutX="147.0" layoutY="320.0" text="Password Incorrect!" textFill="RED">
         <font>
            <Font size="10.0" />
         </font>
      </Label>
      <Button layoutX="149.0" layoutY="67.0" mnemonicParsing="false" onAction="#btnAddNewUserOnAction" prefHeight="35.0" prefWidth="135.0" text="+Add New User" />
      <Label fx:id="lblID" layoutX="196.0" layoutY="110.0" prefHeight="9.0" prefWidth="87.0" text="User ID" />
   </children>
</AnchorPane>
